<div class="modal-body">
    <div class="info-text">@((MarkupString)ErrorDesc)</div>
    <ul>
        @foreach(var error in ErrorDetails)
        {
            <li>@((MarkupString)error)</li>
        }
    </ul>

</div>
<div class="modal-footer">
    <div class="container-fluid ">
        <div class="row">
            <div class="col-6 text-left">
                <button type="button" data-dismiss="modal" @onclick="@Close" class="btn btn-secondary">Cancel</button>
            </div>

        </div>
    </div>
</div>

