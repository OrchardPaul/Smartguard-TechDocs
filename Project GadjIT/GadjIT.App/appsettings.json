{
  "ConnectionStrings": {
    "OR_GadjIT_Web": "Data Source=dev\\SQLEXPRESS;Initial Catalog=OR_GadjIT_Web;Integrated Security=True;Pooling=False"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=dev\\SQLEXPRESS;Initial Catalog=OR_GadjIT_Web;Integrated Security=True;Pooling=False",
          "tableName": "GadjIT_Log",
          "autoCreateSqlTable": true
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "F:\\WorkSpaces\\JD\\GadjIT Log\\Log.txt"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByExcluding",
                "Args": {
                  "expression": "StartsWith(SourceContext, 'Microsoft.')"
                }
              }
            ]
          }
        }
      }

    ]
  },
